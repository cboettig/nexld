{
  "@context": "https://raw.githubusercontent.com/cboettig/nexld/master/inst/context/nexml.json",
  "@type": "nexml",
  "otus": {"@embed": "@always"},
  "trees": {
    "@id": {},
    "otus": { "@embed": "@never"},
    "tree": {
      "@id": {},
      "node": { "@embed": "@always" },
      "edge": {
        "source": {"@embed": "@never"},
        "target": {"@embed": "@never"}
      }
    }
  },
  "characters": {
    "@id": {},
    "otus": {"@embed": "@never"},
    "format": {
      "@embed": "@always",
      "@default": {},
      "states": {
        "@default": {},
        "state":  {
          "@embed": "@always",
          "@default": {}
        },
        "uncertain_state_set": {
          "@id": {},
          "@default": {},
          "member": {"@id": {}, "@default": {}, "state": {"@embed": "@never"}}
        },
        "polymorphic_state_set": {
          "@id": {},
          "@default": {},
          "member": {"@id": {}, "@default": {}, "state": {"@embed": "@never"}}
        }
      },
      "char": {
        "@embed": "@always",
        "@default": {},
        "@id": {},
        "states": {"@default": {}, "@embed": "@never"}
      }
    },
    "matrix": {
      "@id": {},
      "row": {
        "@id": {},
        "otu": {"@embed": "@never"},
        "cell": {
          "@id": {},
          "@default": {},
          "char": {
            "@id": {},
            "states": { "@embed": "@never"}
          },
          "state": {"@embed": "@never"}
        }
      }
    }
  }
}
